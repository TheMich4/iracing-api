{
  "name": "iracing-api-js",
  "version": "0.0.3",
  "description": "Javascript client for iracing API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "npx rome format src/ --write",
    "prepare": "npm run build",
    "prepublishOnly": "npm run format",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "patch": "npm version patch"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheMich4/iracing-api-js.git"
  },
  "author": "Michal Dyczkowski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheMich4/iracing-api-js/issues"
  },
  "homepage": "https://github.com/TheMich4/iracing-api-js#readme",
  "dependencies": {
    "axios": "^1.3.2",
    "axios-cookiejar-support": "^4.0.6",
    "crypto-js": "^4.1.1",
    "tough-cookie": "^4.1.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.13.0",
    "@types/tough-cookie": "^4.0.2",
    "rome": "^11.0.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "iracing",
    "api",
    "client",
    "javascript"
  ]
}
