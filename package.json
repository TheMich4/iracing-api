  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheMich4/iracing-api-js/issues"
  },
  "homepage": "https://github.com/TheMich4/iracing-api-js#readme",
  "devDependencies": {
    "rome": "^11.0.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "iracing",
    "api",
    "client",
    "javascript"
  ]
